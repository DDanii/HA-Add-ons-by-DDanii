{
   "name": "Kasm Workspaces - DEVELOPMENT",
   "version": "2024.09",
   "slug": "kasm",
   "image": "ghcr.io/ddanii/kasm-{arch}",
   "description": "Browser-based access to desktops, applications, and web services",
   "url": "https://github.com/DDanii/HA-Add-ons-by-DDanii/tree/master/kasm",
   "map": [
      "ssl",
      "addon_config:rw"
   ],
   "environment": {
      "KASM_PORT": 35489
   },
   "init": false,
   "options": {
      "ssl": false,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
   },
   "schema": {
      "ssl": "bool",
      "certfile": "str",
      "keyfile": "str"
   },
   "ports": {
      "3000/tcp": 3000,
      "35489/tcp": 35489
   },
   "ports_description": {
      "3000/tcp": "Setup wizard port",
      "35489/tcp": "Kasm port (don't change)"
   },
   "ingress": true,
   "ingress_port": 35489,
   "ingress_stream": true,
   "arch": [
      "aarch64",
      "amd64"
   ]
}

{
  "name": "openldap - DEVELOPMENT",
  "version": "1.5.0",
  "slug": "openldap",
  "image": "ghcr.io/ddanii/openldap-{arch}",
  "description": "OpenLDAP based on osixia container",
  "url": "https://github.com/DDanii/HA-Add-ons-by-DDanii/tree/master/openldap",
  "map": [
    "ssl"
  ],
  "init": false,
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str"
  },
  "ports": {
    "389/tcp": 389,
    "636/tcp": 636
  },
  "ports_description": {
    "389/tcp": "LDAP",
    "636/tcp": "LDAPS"
  },
  "arch": [
    "armv7",
    "aarch64",
    "amd64"
  ]
}

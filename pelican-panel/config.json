{
  "name": "Pelican Panel - DEVELOPMENT",
  "version": "main-1",
  "slug": "pelican-panel",
  "image": "ghcr.io/ddanii/pelican-panel-{arch}",
  "description": "Game server control panel",
  "url": "https://github.com/DDanii/HA-Add-ons-by-DDanii/tree/master/pelican-panel",
  "map": [
    {
      "type": "addon_config",
      "path": "/pelican-data",
      "read_only": false
    }
  ],
  "environment": {
    "XDG_DATA_HOME": "/pelican-data"
  },
  "init": false,
  "options": {
    "APP_URL": "main",
    "ADMIN_EMAIL": "main"
  },
  "schema": {
    "APP_URL": "str",
    "ADMIN_EMAIL": "str"
  },
  "ports": {
    "80/tcp": 80,
    "443/tcp": 443
  },
  "ports_description": {
    "80/tcp": "Web interface http",
    "443/tcp": "Web interface https"
  },
  "arch": [
    "aarch64",
    "amd64"
  ]
}

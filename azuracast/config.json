{
   "name": "AzuraCast - DEVELOPMENT",
   "version": "2024.09-test7",
   "slug": "azuracast",
   "image": "ghcr.io/ddanii/azuracast-{arch}",
   "description": "self-hosted, all-in-one web radio management suite",
   "url": "https://github.com/DDanii/HA-Add-ons-by-DDanii/tree/master/azuracast",
   "map": [
      "ssl",
      "addon_config:rw"
   ],
   "environment": {
      "APPLICATION_ENV": "production",
      "COMPOSER_PLUGIN_MODE": "false",
      "AUTO_ASSIGN_PORT_MIN": "8000",
      "AUTO_ASSIGN_PORT_MAX": "8499",
      "SHOW_DETAILED_ERRORS": "true",
      "MYSQL_PASSWORD": "azur4c457"
   },
   "init": false,
   "options": {
      "MARIADB_ROOT_PASSWORD": "secret",
      "ssl": false,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
   },
   "schema": {
      "MARIADB_ROOT_PASSWORD": "password",
      "ssl": "bool",
      "certfile": "str",
      "keyfile": "str"
   },
   "ports": {
      "80/tcp": 80,
      "443/tcp": 443,
      "8000/tcp": 8000
   },
   "ports_description": {
      "80/tcp": "Web interface http",
      "443/tcp": "Web interface https",
      "8000/tcp": "8000"
   },
   "ingress": true,
   "ingress_port": 80,
   "ingress_stream": true,
   "arch": [
      "aarch64",
      "amd64"
   ]
}

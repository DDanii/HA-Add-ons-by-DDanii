{
  "name": "AzuraCast - DEVELOPMENT",
  "version": "0.20.4",
  "slug": "azuracast",
  "image": "ghcr.io/ddanii/azuracast-{arch}",
  "description": "self-hosted, all-in-one web radio management suite",
  "url": "https://github.com/DDanii/HA-Add-ons-by-DDanii/tree/master/azuracast",
  "map": [
    "ssl",
    "addon_config:rw",
    "media:rw",
    "share:rw"
  ],
  "services": [
    "mysql:need"
  ],
  "environment": {
    "AZURACAST_STATION_PORTS": "8000,8005,8006,8010"
  },
  "init": false,
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "REDIS_HOST": "localhost",
    "REDIS_PORT": 6379
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "reset_database": "bool?",
    "REDIS_HOST": "str",
    "REDIS_PORT": "port"
  },
  "ports": {
    "80/tcp": 80,
    "443/tcp": 443,
    "2022/tcp": 2022,
    "8000/tcp": 8000,
    "8005/tcp": null,
    "8006/tcp": null,
    "8010/tcp": null
  },
  "ports_description": {
    "80/tcp": "Web interface http",
    "443/tcp": "Web interface https",
    "2022/tcp": "sftp port",
    "8000/tcp": "8000",
    "8005/tcp": "8005",
    "8006/tcp": "8006",
    "8010/tcp": "8010"
  },
  "webui": "http://[HOST]:[PORT:80]",
  "arch": [
    "aarch64",
    "amd64"
  ]
}
